{
  "name": "learncraft-spanish",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "This project was bootstrapped with Create React App.",
  "author": "",
  "license": "ISC",
  "homepage": "https://app.learncraftspanish.com",
  "keywords": [],
  "main": "index.tsx",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "mutate": "stryker run",
    "predeploy": "pnpm run build",
    "preview": "vite preview",
    "refresh-mockdata": "tsx .github/workflows/update-cache.ts",
    "scss": "sass --watch src/components/Coaching/styles:src/components/Coaching/styles",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:hexagon": "vitest --config vitest.config-hexagon.ts --coverage",
    "test:hexagon:watch": "vitest --config vitest.config-hexagon.ts --watch",
    "typecheck": "tsc --noEmit",
    "update-shared": "pnpm add @LearnCraft-Spanish/shared@latest",
    "validate": "npm run lint:fix && npm run format && npm run typecheck",
    "metrics": "node METRICS/get_metrics.js"
  },
  "dependencies": {
    "@LearnCraft-Spanish/shared": "npm:@learncraft-spanish/shared@^0.2.24",
    "@auth0/auth0-react": "^2.3.0",
    "@sentry/cli": "^2.42.2",
    "@sentry/react": "^9.2.0",
    "@sentry/vite-plugin": "^3.2.1",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/utils": "^8.32.0",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-howler": "^5.2.0",
    "react-router-dom": "^6.30.0",
    "react-toastify": "^11.0.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "web-vitals": "^4.2.4",
    "zod": "^3.24.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.14.0",
    "@antfu/eslint-config": "^4.3.0",
    "@eslint-react/eslint-plugin": "^1.27.0",
    "@faker-js/faker": "^9.7.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/jest-runner": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@stryker-mutator/vitest-runner": "^8.7.1",
    "@tanstack/eslint-plugin-query": "^5.66.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/eslint-plugin": "^1.1.32",
    "axios": "^1.8.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-vitest": "^0.5.4",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "prettier": "^3.5.2",
    "sass-embedded": "^1.85.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "vite": "^6.2.6",
    "vitest": "^3.0.7"
  },
  "directories": {
    "test": "tests"
  }
}
