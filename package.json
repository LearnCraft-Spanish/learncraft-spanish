  "homepage": "https://app.learncraftspanish.com",
  "dependencies": {
    "@auth0/auth0-react": "^2.0.1",
    "@sentry/cli": "^2.31.0",
    "@sentry/react": "^7.110.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-howler": "^5.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^2.1.3",
    "save-dev": "^0.0.1-security",
    "typescript": "^5.4.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts build && npm run sentry:sourcemaps",
    "test": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts test",
    "test-vitest": "vitest",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org learncraft-spanish --project learncraft-spanish-frontend ./src && sentry-cli sourcemaps upload --org learncraft-spanish --project learncraft-spanish-frontend ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "vitest": "^1.6.0"
  },
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
